&INIT_NAMELIST_VERSION
      gridvernum    =      6,                                ! grid version number, used to distinguish grids in setupgrid 
/
&INIT_GRID_DESCRIPTION 
     gcmname        = 'NEMO-ORCA1',                          ! name of GCM            
     gcmsource      = 'http://www.nemo-ocean.eu/',           ! website for GCM
     gridname       = 'ORCA1.L75',                           ! name of grid
     gridsource     = 'http://www.nemo-ocean.eu/',           ! website for grid
     griddesc       = 'Global 1 deg run',                    ! short description of grid
     ! Describe filenames
     topoDataDir    = '/Users/jkjellsson/Downloads/ORCA1-N406/',  ! directory of grid data
     RunID          = 'ORCA1-N406',                          ! name of GCM simulation
     physDataDir    = '/Users/jkjellsson/Downloads/ORCA1-N406/',  ! directory of physical data (u,v,T,S,MLD,etc)
     physPrefixForm = 'RUNID_YYYYGRIDX'                      ! format of filenames for physical data
     bioDataDir     = '',                                    ! directory of biogeochemistry data 
     bioPrefixForm  = '',                                    ! format of filenames for BGC data
     tGridName      = 'T',                                   ! name of Tgrid, e.g. grid_T
     uGridName      = 'U',                                   ! name of Ugrid
     vGridName      = 'V',                                   ! name of Vgrid
     fileSuffix     = '.nc4',                                ! suffix of files, e.g. ".nc"
     ! Describe variable names
     ssh_name       = 'sossheig',                            ! name of SSH
     ueul_name      = 'vozocrtx',                            ! name of resolved zonal velocity
     veul_name      = 'vomecrty',                            ! ---- " -------   meridional 
     usgs_name      = 'vozoeivu',                            ! name of sub-grid zonal velocity, e.g. GM for ORCA1
     vsgs_name      = 'vomeeivv',                            ! ---- " -------   meridional 
     temp_name      = 'votemper',                            ! name of temperature
     salt_name      = 'vosaline',                            ! name of salinity
     ! Logicals to control what to read etc.
     oneStepPerFile = .false.,                               ! only one time level per file?
     readMean       = .false.,                               ! read stored time mean velocities
     readTS         = .true.,                                ! read temperature and salinity
     vvl            = .true.,                                ! variable volume layer?
     readBio        = .false.,                               ! read biogeochemistry fields
     readSSH        = .true.,                                ! read SSH (necessary for vvl)
     sgsUV          = .true.,                                ! read sub-grid velocities, e.g GM. for ORCA1
     useTrmClock    = .false.,                               ! use sophisticated clock (otherwise simple clock)
/
&INIT_CASE_DESCRIPTION
     casename       = 'ORCA1',                               ! short name of the TRACMASS run
     casedesc       = 'Test',                                ! short description of the TRACMASS run
/
&INIT_GRID_SIZE
     ! Size of global grid
     imt            = 362,                                   ! number of global i points
     jmt            = 292,                                   ! number of global j points
     km             = 75,                                    ! number of global k points
     nst            = 2,                                     ! number of time levels to use (should be = 2)
     ! Only read subregion of the global data
     subgrid        = 0,                                     ! only read part of the grid if > 0
     subgridimin    = 3529,                                  ! i start point of region
     subgridimax    = 3725,                                  ! i end point of region
     subgridjmin    = 2279,                                  ! j start point of region
     subgridjmax    = 2634,                                  ! j end point of region
     subgridfile    = 'templ.asc',                           ! or read region indices from file
     subgridid      = 0,                                     ! number for this region
     
     nperio         = 0,                                     ! control periodic boundary conditions
                                                             ! > 0 periodic in x
                                                             ! = 4 North fold for ORCA2, 025, 0083
                                                             ! = 6 North fold for ORCA1
     coordFile      = 'mesh_hgr.nc'                          ! file with lons,lats
     hgridFile      = 'mesh_hgr.nc'                          ! file with dx, dy etc
     zgridFile      = 'mesh_zgr.nc'                          ! file with dz etc.
     bathyFile      = 'bathy_level.nc'                       ! file with bathymetry in model levels
     dx_name        = 'e1t'                                  ! name of dx at T point
     dy_name        = 'e2t'                                  !         dy at T point
     dxv_name       = 'e1v'                                  !         dx at V point
     dyu_name       = 'e2u'                                  !         dy at U point
     dz_1D_name     = 'e3t_0'                                ! name of 1D dz array at T point
     dzt_3D_name    = 'e3t'                                  ! name of 3D dz at T point
     dzu_3D_name    = 'e3u'                                  ! --- " ---  dz at U point
     dzv_3D_name    = 'e3v'                                  ! --- " ---  dz at V point
     kBathy_name    = 'Bathy_level'                          ! name of bathymetry in model levels
     gridIsUpsideDown = .true.                               ! flip vertical grid?
     read3Ddz       = .true.                                 ! read 3D dz for T,U,V points?
/
&INIT_BASE_TIME
     basesec        = 0,                                     ! base time and date for trmClock
     basemin        = 0,
     basehour       = 0,
     baseday        = 1,
     basemon        = 1,
     baseyear       = 1,
     jdoffset       = 4,                                     ! trmClock offset in juliandays
/
&INIT_GRID_TIME
     fieldsperfile  = 12,
     ngcm_step      = 1,                                     ! time between two time levels
                                                             ! if ncgcm_unit = 5, then ngcm = 3 means 3 months
     ngcm_unit      = 5,                                     ! units of ngcm 
                                                             ! 1 = seconds
                                                             ! 2 = minutes
                    					     ! 3 = hours
							     ! 4 = days
							     ! 5 = months
							     ! 6 = years
     
     iter           = 10,                                    ! number of time steps between two GCM time levels
     intmax         = 15330,                                 ! max iterations before trajectory is 
                                                             ! stopped with infiniteError
     minveljd       = 1,                                     !
     maxveljd       = 120,                                   !
/
&INIT_START_DATE
     startsec       = 0,                                     ! start time
     startmin       = 0,
     starthour      = 0,
     startday       = 1,                                     ! start date
     startmon       = 1,
     startyear      = 2000,
     noleap         = .true.,                                ! dont use leap years? 
     startjd        = -1,                                    ! first julianday for trmClock
     intmin         = 1,                                     !
/
&INIT_RUN_TIME
     ! Loop run 
     loopYears      = .true.,				     ! make a time loop to run 
     		      					     ! e.g. 1000 years using only 100 years of data
     loopStartYear  = 2000,				     ! year to start loop from
     loopEndYear    = 2001,				     ! last year of loop
     
     intspin        = 12,                                    ! number of time steps for seeding
     intrun         = 120,                                   ! number of time steps for run
     log_level      = 0,				     ! level of verbosity 
     		      					     ! = 0 minimal output to stdout
							     ! = 1 low verbosity
							     ! = 2 medium verbosity
/
&INIT_WRITE_TRAJS
     twritetype     = 11,                                    ! how to write time 
                                                             ! = 1 write float time variable
  							     ! = 2 write julianday
							     ! > 2 write time step
     kriva          = 1,                                     ! 0 = no writing
             	      					     ! 1 = write at time intervals of gcm datasets (each ints)
        						     ! 2 = write at each time iteration
							     ! 3 = write all the time
  							     ! 4 = write only start and end positions
  							     ! 5 = write at chosen intervals
  							     ! 6 = write each spatial grid-crossing
     outdatadir     = '/Users/jkjellsson/Downloads/',        ! directory for output
     outdatafile    = 'orca1_test',			     ! fileprefix for output
     intmininoutfile= 0,				     !
     intpsi         = 10, 									! frequency of writing the stream functions
/
&INIT_SEEDING
     nff            = 1,                                     ! = 1 run forward trajectories
     		      					     ! = -1 run backwards
     isec           = 2,				     ! = 1 start on zonal cell wall 
     		      					     ! = 2 start on meridional cell wall
							     ! = 3 start on vertical wall
							     ! = 4 start in center of cell
     idir           = 0,				     ! = 1 start only when flux > 0
     		      					     ! = -1      only when flux < 0 
							     ! = 0 start at flux < 0 or > 0
     nqua           = 1,
     partquant      = 1,
     ntracmax       = 20000000,			             ! max number of trajectories
     loneparticle   = 0, 				     ! start only one trajectory for debugging
     seedtype       = 1,				     ! = 1 seed using ist,jst,kst (below)
     		      					     ! = 2 seed using file with start positions
							     
							     	     ! when seedtype = 1
     ist1           = 235,				     ! seed in box in range i = [ist1,ist2]
     ist2           = 237,				     ! 	       	      	    j = [jst1,jst2]
     jst1           = 198,				     ! 			    		k = [kst1,kst2]
     jst2           = 198,				     
     kst1           = 70,
     kst2           = 75,
     tst1           = -1,				     ! seed only between tst1 and tst2
     tst2           = -1,				     ! given as 19800105 for 1980-01-05
     
     		      					     ! when seedtype = 2
     seeddir        = '',				     ! directory for seed files 
     seedfile       = '',				     ! name of seed file
     varseedfile    = 0,				     ! varying names for seedfiles
     seedtime       = 0,				     ! seed only for specific times
     seedall        = 0,				     ! seed all particles or only one
     seedpos        = 1,				     ! 
     seedparts      = 0,				     !
     seedpart_id    = 0,				     !
/
&INIT_KILLZONES
     ! If a trajectory enters a killzone it will be killed
     ! ienw,iene are western and eastern i indices of killzone
     ! jens,jenn are southern and northern j indices of killzone
     ! ienw =   0,  500,
     ! iene = 100, 1000,
     ! jens = 200,  450,
     ! jenn = 300,  470,
     ! sets up two killzones, the first i = [0,100], j = [200,300]
     ! the other i = [500,1000], j = [450,470]

     nend           = 5,                                     ! number of ways to kill a traj
     ienw           =   -1, 2402,   -1,   -1,		     ! west indices of killzones
     iene           =    2, 3000, 3000, 3000,		     ! east indices of killzones
     jens           =    1,    1,    1, 1481,		     ! south indices of killzones
     jenn           = 2000, 2000,    2, 2000,		     ! north indices of killzones
     timax          = 3650000.0,     			     ! time limit for traj [days]
/
&INIT_TEMP_SALT
     ! Start or end trajectories in tracer intervals
     tmin0          =  -50.0,                                ! start only if T >= tmin0
     tmax0          =  400.0,				     ! ---- " ----   T <= tmax0      
     smin0          = -500.0,				     ! ---- " ----   S >= smin0
     smax0          =  400.0,				     !               S <= smax0
     rmin0          = -100.0,				     ! 		   rho >= rmin0
     rmax0          =  500.0,				     !  	   rho <= rmax0
     tmine          =  -50.0,				     ! kill traj  if T <  tmine
     tmaxe          =  400.0,				     ! ---- " -----  T >  tmaxe
     smine          = -150.0,				     !               S <  smine
     smaxe          =  500.0,				     ! 		     S >  smaxe
     rmine          = -100.0,				     ! 		   rho <  rmine
     rmaxe          =  500.0,				     !		   rho >  rmaxe
/
&INIT_TRACERS
     n2Dtracers = 1
     names2Dtracers(1) = 'sossheig' 
     n3Dtracers = 2     
     names3Dtracers(1:2) = 'votemper','vosaline'
/
&INIT_DIFFUSION
     ! Add diffusion (random walk) to trajectories
     ah             = 0.0,				     ! horizontal diffusivity
     av             = 0.0,				     ! vertical diffusivity
/
&INIT_SEDIMENT
     ! For sedimentation parametrization (not maintained anymore)
     partdiam       = 0.001,		      		     !
     rhos           = 2620.0,				     !
     cwamp          = 20.0,				     !
     twave          = 8.0,				     !
     critvel        = 0.1,				     !
/
