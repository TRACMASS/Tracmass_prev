            jst1 =      35,  !215, !1,    !16, !65, !64,   !115, !168, !187, !115, !236, !115, !236, ! 97,115, 1150, ! 650,
            jst2 =      90,  !292, !200,  !100, !65, !64,   !115, !168, !187, !115, !236, !115, !236, ! 97,115, !1159, ! 659,
            kst1 =      1, !35, !30,
