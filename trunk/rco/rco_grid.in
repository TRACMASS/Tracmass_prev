  &INITGRIDDESC
   GCMname    = 'rco',
   GCMsource  = 'rco',
   gridName   = 'rco',
   gridSource = 'rco',
   gridDesc   = '2 minutes run for the Baltic Sea'/

$INITGRIDGRID
  IMT=320,
  JMT=362,
  KM=41,
  LBT=1,
  NEND=2/       !NEND = LBT +1

$INITGRIDNTRAC
